*.perl-ext-common: default,matcher
*.font: xft:JetBrainsMono Nerd Font Mono:size=11:antialias=true
*.scrollBar: false
*.clipboard.autocopy: true
*.keysym.M-c: perl:clipboard:copy
*.keysym.M-v: perl:clipboard:paste
*.url-launcher: /usr/bin/xdg-open
*.matcher.button: 1
*.print-pipe: "cat > /dev/null"
*.selectToClipboard: true
